<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ccv_qweb_template_res_config_settings_view_form" model="ir.ui.view">
        <field name="name">confirm_order_process.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@id='inter_company']" position="after">
                <div id="key" class="col-12 col-lg-6 o_setting_box" title="Cấu hình Serect key.">
                    <div class="o_setting_right_pane">
                        <span>Cấu hình key cho người dùng không đăng nhập</span>
                        <div>
                            <button icon="fa-refresh" type="object" name='reset_serect_key' string="Reset serect key"/>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
