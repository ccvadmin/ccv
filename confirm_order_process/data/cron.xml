<?xml version='1.0' encoding='utf-8'?>
<odoo>
<data noupdate="1">
    <record id="reset_serect_key_for_public_website" model="ir.cron">
        <field name="name">Reset Serect key for Public Website</field>
        <field name="model_id" ref="model_order_link"/>
        <field name="state">code</field>
        <field name="code">model.reset_serect_key()</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now().replace(hour=0,minute=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>
    <record id="ir_cron_otp_verify" model="ir.cron">
        <field name="name">OTP : Delete the verified otps generated </field>
        <field name="model_id" ref="model_otp_verification"/>
        <field name="state">code</field>
        <field name="code">model._cron_delete_verified_otp()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">weeks</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
    <record id="ir_cron_otp_expired" model="ir.cron">
        <field name="name">OTP : Update the expired otps </field>
        <field name="model_id" ref="model_otp_verification"/>
        <field name="state">code</field>
        <field name="code">model.update_expired_otps()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
</data>
</odoo>
